local Players = game:GetService("Players")
local Player = Players.LocalPlayer

local whitelist = {
	'TradeScomTest',
	'Nuknaime_YT43',
} 

if table.find(whitelist, Player.Name) then
	game.Players.TradeScomTest.Stat.Units.Unit2.Value = "Golden minigunner"
	game.Players.TradeScomTest.Stat.Units.Unit3.Value = "Golden repulsor"
	game.Players.TradeScomTest.Stat.Units.Unit4.Value = "Diamond pistoler"
else
	while true do
	print("idi nahyu, hwid issue")
	wait(0.1)
	end
end
